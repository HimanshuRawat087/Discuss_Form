<h2>Topics</h2>

<ul class="collection">
  <li  :for={topic <- @topic} class="collection-item">
      <%= topic.title %>
      <div class="right">
        <a href={"/topics/#{topic.id}/edit"}>
          Edit </a>
      </div>
    </li>
</ul>


<div class="fixed-action-btn">
 <a href="/topics/new" class="btn-floating btn-large waves-effect waves-light red">
    <i class="material-icons">add</i>
 </a>
</div>

